<body>
  <!--page title-->
  <div class="dzsparallaxer auto-init height-is-based-on-content "  data-options='{   direction: "reverse"}'
       data-responsive-reference-width="1280" data-responsive-optimal-height="500">
    <div class="divimage dzsparallaxer--target" style="width: 100%; height: 100%; background-image: url(<%= image_path('hero2.jpg')%>)">
    </div>

    <div class="container pt100 pb70">
        <div class="row pb50">
            <div class="col-md-8 ml-auto mr-auto wow bounceIn" data-wow-delay=".2s">
                <h3 class="h1 text-center font500  pt100 text-black"></h3>
            </div>
        </div>
    </div>
  </div><!--parallax-->
  <!--/.page-title-->

  <section class="faq container" aria-level="Frequently Asked Questions">
    <header class="faq_header">
      <h2 class="faq_header-title">Frequently Asked Questions</h2>
    </header>
    <div class="faq__body">
      <% @faqs.each_with_index do |faq, index| %>
        <details aria-expanded="true" class="faq__panel">
          <summary class="faq__label"><%= faq.question %></summary>
          <div class="faq__panel-body">
            <p class="faq__panel-answer"><%= faq.answer %></p>
          </div>
        </details>
      <% end %>
    </div>
  </section>
</body>
